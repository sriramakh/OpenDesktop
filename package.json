{
  "name": "open-desktop",
  "version": "1.0.0",
  "description": "Local-first desktop agent that observes, plans, and executes multi-step tasks across the OS",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:renderer\" \"wait-on http://localhost:5173 && electron .\"",
    "dev:renderer": "vite --config vite.config.js",
    "build": "vite build --config vite.config.js && electron-builder",
    "lint": "eslint src/"
  },
  "keywords": [
    "desktop-agent",
    "ai",
    "automation",
    "electron",
    "local-first"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "electron": "^28.1.3",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "electron-store": "^8.1.0",
    "exceljs": "^4.4.0",
    "glob": "^10.3.10",
    "jszip": "^3.10.1",
    "lucide-react": "^0.312.0",
    "mammoth": "^1.11.0",
    "marked": "^11.1.1",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^2.4.5",
    "playwright": "^1.41.1",
    "pptxgenjs": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  }
}
